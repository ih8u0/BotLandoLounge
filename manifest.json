{
  "manifest_version": 3,
  "name": "BotLando",
  "description": "Bot for Zalando Lounge",
  "version": "1.0.0",
  "content_scripts": [
  {
    "matches": ["https://www.zalando-lounge.pl/campaigns/*"],
    "css": ["style.css"],
    "js": ["script.js"]
  },
  {
    "matches": ["https://www.zalando-lounge.pl/campaigns/*/categories/*/articles/*"],
    "js": ["articleScript.js"],
    "css": ["articleStyles.css"]
  }
  ]
}